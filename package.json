{
  "name": "graphql-sdk-generator",
  "version": "0.0.0",
  "description": "GraphQL SDK generator translates GraphQL Schema to code enabling you to get smooth auto completion and validation for your GraphQL Schema",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "bin": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "rm -rf build && tsc",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm format",
      "pnpm lint"
    ]
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siddharth9890/graphql-sdk-generator.git"
  },
  "keywords": [
    "graphql-sdk-generator",
    "graphql",
    "sdk",
    "generator",
    "typescript",
    "graphql-generator"
  ],
  "author": "Siddharth <siddharth.a9890@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-tools/utils": "^10.3.2",
    "ajv": "^8.17.1",
    "commander": "^12.1.0",
    "graphql": "^16.9.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "execa": "^9.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node-dev": "^2.0.0"
  }
}
